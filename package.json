{
  "name": "wiremock-captain",
  "version": "3.3.1",
  "description": "A better way to use the WireMock simulator to test your HTTP APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/HBOCodeLabs/wiremock-captain.git"
  },
  "license": "MIT",
  "keywords": [
    "api",
    "captain",
    "integration",
    "jest",
    "test",
    "testing",
    "typescript",
    "wiremock"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && eslint .",
    "clean": "rimraf coverage dist tmp .buildcache",
    "test": "JEST_JUNIT_OUTPUT_NAME=unit-test.xml jest ./test/unit",
    "integration-test": "JEST_JUNIT_OUTPUT_NAME=integration-test.xml jest ./test/integration/ --runInBand",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ncu:check": "ncu",
    "ncu:make": "ncu -u",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write --loglevel silent ."
  },
  "precommit": [
    "precommit"
  ],
  "engines": {
    "node": ">=18.20.2",
    "npm": ">=10.5.0"
  },
  "dependencies": {
    "axios": "1.6.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "eslint": "9.4.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unused-imports": "4.0.0",
    "express": "4.19.2",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "npm-check-updates": "16.14.20",
    "pre-commit": "1.2.2",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "rimraf": "5.0.7",
    "ts-jest": "29.1.4",
    "tslib": "2.6.2",
    "typescript": "5.4.5"
  }
}
