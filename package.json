{
  "name": "wiremock-captain",
  "version": "3.3.0",
  "description": "A better way to use the WireMock simulator to test your HTTP APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/HBOCodeLabs/wiremock-captain.git"
  },
  "license": "MIT",
  "keywords": [
    "api",
    "captain",
    "integration",
    "jest",
    "test",
    "testing",
    "typescript",
    "wiremock"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && eslint .",
    "clean": "rimraf coverage dist tmp .buildcache",
    "test": "JEST_JUNIT_OUTPUT_NAME=unit-test.xml jest ./test/unit",
    "integration-test": "JEST_JUNIT_OUTPUT_NAME=integration-test.xml jest ./test/integration/ --runInBand",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ncu:check": "ncu",
    "ncu:make": "ncu -u",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write --loglevel silent ."
  },
  "precommit": [
    "precommit"
  ],
  "engines": {
    "node": ">=16.20.0",
    "npm": ">=8.19.4"
  },
  "dependencies": {
    "axios": "1.4.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "express": "4.18.2",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "npm-check-updates": "16.10.12",
    "pre-commit": "1.2.2",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.0",
    "tslib": "2.6.1",
    "typescript": "5.0.4"
  }
}
